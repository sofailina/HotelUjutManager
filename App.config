<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="entityFramework"
				 type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0" />
	</configSections>

	<connectionStrings>
		<add name="HotelUjutConnection"
			 connectionString="Data Source=S\STP;Initial Catalog=HotelUjut;Integrated Security=True"
			 providerName="System.Data.SqlClient" />
	</connectionStrings>

	<entityFramework>
		<providers>
			<provider invariantName="System.Data.SqlClient"
					  type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
		</providers>
	</entityFramework>
</configuration>